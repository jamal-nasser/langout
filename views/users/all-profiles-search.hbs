<div class="container">
  <div class="row">
    {{#each filteredUsers}}

    <div class="col-4">
      <img class="img-fluid" alt="Responsive image" class="user-image" src="{{imageUrl}}" alt="{{fullName}} - image">

      <div class="card-info">
        <h3>{{fullName}}</h3>
        <p>I can teach {{speakingLanguage}} and I want to learn {{learningLanguage}} </p>
        <a href="/contact/{{id}}">Let's connect</a>

      </div>
    </div>
    {{/each}}
  </div>
</div>